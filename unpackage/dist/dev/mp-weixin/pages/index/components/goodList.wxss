.wf-page {
  display: grid;
  grid-template-columns: 1fr 1fr;
  grid-gap: 20rpx;
}
.wf-item {
  width: calc((100vw - 2 * 10px - 10px) / 2);
  padding-bottom: 20rpx;
}
.wf-page1 .wf-item {
  margin-top: 20rpx;
  background-color: #fff;
  border-radius: 20rpx;
  padding-bottom: 0;
}
.item ~ .item {
  margin-top: 20rpx;
}
.info_box {
  padding: 16rpx 20rpx;
  border-radius: 0 0 20rpx 20rpx;
  background-color: #fff;
}
.bg--w111-484643 {
  background: linear-gradient(90deg, #484643 0%, #1F1B17 100%);
}
.text--w111-FDDAA4 {
  color: #FDDAA4;
}
.svip_rd {
  border-radius: 14rpx 0 8rpx 14rpx;
}
.cart-btn {
  width: 100%;
  height: 100%;
}

