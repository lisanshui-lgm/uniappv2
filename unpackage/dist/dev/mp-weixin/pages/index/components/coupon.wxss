.coupon-wrap {
  background: var(--module-color);
}
.coupon1 .list {
  flex-wrap: nowrap;
}
.coupon1 .item {
  flex-shrink: 0;
  position: relative;
  width: 156rpx;
  height: 156rpx;
  border-radius: 32rpx;
  margin-top: 20rpx;
  margin-right: 12rpx;
  background: var(--view-theme);
}
.coupon1 .item .text {
  width: 144rpx;
  height: 156rpx;
  padding-top: 22rpx;
  border: 2rpx solid #FCEAE9;
  border-radius: 16rpx;
  margin: -20rpx auto 0;
  background: #FFFFFF;
}
.coupon1 .item .money {
  height: 40rpx;
  text-align: center;
  font-weight: 500;
  font-size: 28rpx;
  color: var(--view-theme);
}
.coupon1 .item .number {
  font-family: SemiBold;
  font-size: 40rpx;
  line-height: 40rpx;
}
.coupon1 .item .info {
  margin-top: 10rpx;
  text-align: center;
  font-size: 18rpx;
  line-height: 26rpx;
  color: #999999;
}
.coupon1 .item .button {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 0;
  height: 76rpx;
  padding-top: 28rpx;
  border-radius: 0 0 32rpx 32rpx;
  background: linear-gradient(90deg, var(--view-theme) 0%, var(--view-gradient) 100%);
  text-align: center;
  font-weight: 500;
  font-size: 24rpx;
  color: #FFFFFF;
}
.coupon1 .item .button::before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  width: 156rpx;
  height: 20rpx;
  border: 8rpx solid #FCEAE9;
  border-top-color: #FFFFFF;
  border-bottom-right-radius: 78rpx 20rpx;
  border-bottom-left-radius: 78rpx 20rpx;
  box-sizing: border-box;
  background: #FFFFFF;
}
